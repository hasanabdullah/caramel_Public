<!doctype html>
<html>

<head>
    <title>Log In - Caramel</title>
    <link rel="stylesheet" type="text/css" href="/css/modern-styles.css">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Lobster" rel="stylesheet">
    <script src="/fb-login.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
</head>
<body>

<body>

<div class="top-header">
    <div class="header-logo">

    </div>
    <div class="header-navigation">
    </div>
    <div class="header-right" style="width: 200px">
        <p style="float: left;margin-right: 20px"><a href="/shop-login">SHOP LOGIN</a></p>
        <p style="float: left"><a class="active" href="/customer-register">SIGN UP</a></p>
    </div>
</div>

<div class="container-main" style="float: left;width: 50%">
    <h1 style="text-align: left;font-family: inherit; margin-top: 20%;margin-left: 30%;font-size: 24px;margin-bottom: -30px">
        Welcome to</h1>
    <h1 style="text-align: left;font-family: 'Lobster';margin-left: 30%;font-size: 92px;margin-top:0">caramel</h1>
</div>


<div class="container-main" style="float:left; width: 50%">

    <div class="container-body" style="width: 300px">
        <h5 style="font-size: 16px;margin-bottom: 50px;text-transform: none; color: #666; font-weight: 300">Customer
            Sign In</h5>


        <form action="/customer-login" method="post">
            <div class="form-group">
                <input type="text" style="width: 300px" name="email">
                <h4>Email</h4>
            </div>
            <div class="form-group">

                <input type="password" style="width: 300px" name="password">
                <h4>Password</h4>
            </div>
            <button type="submit" class="button-normal">Login</button>

        </form>

        <fb:login-button
                scope="public_profile,email,user_friends"
                onlogin="jump();">Log In
        </fb:login-button>
        <h2 style="font-variant: normal;font-size: 14px; font-weight: 300"><a href="/customer-register" id="redirect">Need
                an account?
                Register</a></h2>
        <h2 style="font-variant: normal;font-size: 14px; font-weight: 300"><a href="/reset/customer">Forgot
                Password?</a></h2>
        <h2 style="font-variant: normal;font-size: 14px; font-weight: 300"><a href="/">Return home</a></h2>
    </div>
</div>
</div>
<div class="white-space"></div>
</body>
</html>
